{
  "name": "make-new-component",
  "version": "2.0.1",
  "description": "CLI for creating new React components",
  "files": [
    "dist",
    "templates"
  ],
  "bin": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "dev": "npm run prepublishOnly",
    "prepublishOnly": "esbuild src/index.ts --platform=node --outfile=dist/index.js --bundle --minify"
  },
  "keywords": [
    "react",
    "reactjs",
    "cli",
    "component"
  ],
  "author": {
    "name": "Eric Puskas"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EricPuskas/make-new-component.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^14.14.37",
    "chalk": "^4.1.0",
    "commander": "^7.2.0",
    "esbuild": "^0.11.4",
    "typescript": "^4.2.3"
  }
}
